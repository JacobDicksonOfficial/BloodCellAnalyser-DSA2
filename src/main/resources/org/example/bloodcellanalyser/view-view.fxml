<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.bloodcellanalyser.controllers.ViewController" spacing="10" alignment="CENTER">

    <Label text="Preview View" />
    <ImageView fx:id="imagePreview" fitWidth="800" fitHeight="600" preserveRatio="true" />
    <Button fx:id="backBtn" text="Back to Main" onAction="#backToMain" />

</VBox>
